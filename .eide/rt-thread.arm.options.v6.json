{
    "version": 3,
    "beforeBuildTasks": [],
    "afterBuildTasks": [],
    "global": {
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-0",
        "language-c": "gnu11",
        "language-cpp": "c++11",
        "warnings": "ac5-like-warnings",
        "one-elf-section-per-function": true,
        "split-load-and-store-multiple": true
    },
    "asm-compiler": {
        "$use": "asm-auto"
    },
    "linker": {
        "output-format": "elf",
        "misc-controls": "--diag_suppress=L6329"
    }
}